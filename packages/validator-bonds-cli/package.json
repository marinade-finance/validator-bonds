{
  "name": "@marinade.finance/validator-bonds-cli",
  "version": "2.2.0",
  "description": "CLI of the validator bonds contract",
  "repository": {
    "type": "git",
    "url": "git@github.com:marinade-finance/validator-bonds.git"
  },
  "publishConfig": {
    "access": "public"
  },
  "keywords": [
    "solana",
    "marinade.finance",
    "blockchain",
    "staking",
    "mnde"
  ],
  "author": "Marinade.Finance",
  "license": "ISC",
  "bin": {
    "validator-bonds": "./dist/src/index.js"
  },
  "main": "dist/src/index.js",
  "types": "dist/src/index.d.ts",
  "scripts": {
    "build": "tsc --build",
    "cli": "pnpm ts-node ./src/",
    "test:validator": "pnpm jest --config ../../jest.config.test-validator.js --testPathPattern=\"$(pwd)\"",
    "test": "pnpm test:validator"
  },
  "files": [
    "dist/",
    "!dist/__tests__",
    "README.md"
  ],
  "dependencies": {
    "@marinade.finance/validator-bonds-sdk": "workspace: *",
    "@marinade.finance/validator-bonds-cli-core": "workspace: *",
    "@coral-xyz/anchor": "0.29.0",
    "@marinade.finance/anchor-common": "4.1.2",
    "@marinade.finance/cli-common": "4.1.2",
    "@marinade.finance/ts-common": "4.1.2",
    "@marinade.finance/web3js-1x": "4.1.2",
    "@marinade.finance/ledger-utils": "3.1.0",
    "@solana/web3.js": "1.98.4",
    "bn.js": "5.2.2",
    "bs58": "6.0.0",
    "commander": "14.0.0",
    "jsbi": "4.3.2",
    "pino": "9.7.0",
    "pino-pretty": "13.0.0",
    "solana-spl-token-modern": "npm:@solana/spl-token@^0.3.11",
    "yaml": "2.8.1"
  },
  "devDependencies": {
    "crypto-js": "4.2.0",
    "@marinade.finance/jest-shell-matcher": "1.1.1"
  },
  "engines": {
    "node": ">=16"
  }
}
